<?xml version="1.0" encoding="utf-8"?>
<com.daimajia.swipe.SwipeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <!-- Button Delete -->
    <LinearLayout
        android:id="@+id/dragBtnDelete"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:weightSum="1"
        android:background="#ff1a1a"
        android:layout_gravity="right"
        android:orientation="horizontal"
        android:gravity="center|center_vertical">
        <TextView
            style="@style/tv_in_item_lv_ds_tien_nghi_va_loai_phong"
            android:text="@string/btn_xoa"/>
    </LinearLayout>
    <!-- Button Delete End -->


    <LinearLayout
        style="@style/parent_item_lv_ds_tien_nghi_va_loai_phong">
        <TextView
            android:id="@+id/tvMaLoaiPhong"
            style="@style/tv_in_item_lv_ds_tien_nghi_va_loai_phong"/>
    </LinearLayout>

    <LinearLayout
        style="@style/parent_item_lv_ds_tien_nghi_va_loai_phong">
        <TextView
            android:id="@+id/tvLoaiPhong"
            style="@style/tv_in_item_lv_ds_tien_nghi_va_loai_phong"/>
    </LinearLayout>
</com.daimajia.swipe.SwipeLayout>